                            <div class="col-md-8">
                                <div class="tab-content dashboard-content">
                                    
                                    <div class="tab-pane fade active show" id="orders" role="tabpanel" aria-labelledby="orders-tab">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5 class="mb-0">Your Orders</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="table-responsive">
                                                  

                                               <!-- Notification area for success/failure messages -->
                                                <div id="notification" style="display: none;"></div>

                                                <table class="table">
                                                    <thead>
                                                        <tr>
                                                            <th>Product Name</th>
                                                            <th>Product Image</th>
                                                            <th>Status</th>
                                                            <th>Price</th>
                                                            <th>Quantity</th>
                                                            <th>Date</th>
                                                            <th>Actions</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <% orders.forEach(order => { %>
                                                            <% order.items.forEach(item => { %>
                                                                <tr>
                                                                    <td><%= item.product ? item.product.name : 'Product not found' %></td>
                                                    
                                                                    <td>
                                                                        <% if (item.product && item.product.colors && item.product.colors[item.color]) { %>
                                                                            <img src="/uploads/<%= item.product.colors[item.color].images[0] %>" alt="<%= item.product.name %>" style="width: 50px; height: auto;">
                                                                        <% } else { %>
                                                                            <p>Image not available</p>
                                                                        <% } %>
                                                                    </td>
                                                    
                                                                    <!-- Status with conditional styling -->
                                                                    <td>
                                                                        <span style="color: 
                                                                            <%= item.status === 'Cancelled' || order.status === 'Cancelled' ? 'red' : 
                                                                                (item.status === 'Delivered' || order.status === 'Delivered' ? 'green' : 'black') %>;">
                                                                            <%= item.status || order.status %>
                                                                        </span>
                                                                    </td>
                                                    
                                                                    <td>Rs.<%= item.price.toFixed(2) %></td>
                                                    
                                                                    <td><%= item.quantity %></td>
                                                    
                                                                    <td><%= order.createdAt ? order.createdAt.toDateString() : 'Date not available' %></td>
                                                    
                                                                    <td>
                                                                        <a href="/user/order/details/<%= order._id %>" class="text-info">View</a>
                                                                    </td>
                                                                </tr>
                                                            <% }) %>
                                                            <!-- Row for displaying total price of each order -->
                                                            <tr>
                                                                <td colspan="5" style="text-align: right;"><strong>Total Price:</strong></td>
                                                                <td colspan="2">Rs.<%= order.totalPrice.toFixed(2) %></td>
                                                            </tr>
                                                        <% }) %>
                                                    </tbody>
                                                </table>
                                                
                                                
                                                
                                                
                                                
                                                

                                               

                                                    
                                                    
                                                    
                                                </div>
                                            </div>
                                        </div>
