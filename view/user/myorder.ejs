    <script>
        let actionUrl = '';
    
        function openItemModal(orderId, itemId, actionType) {
            // Determine the action URL and text based on the action type
            actionUrl = `/user/${actionType}Item/${orderId}/${itemId}`;
            console.log(actionUrl,'aaaaa');
            
            document.getElementById('actionText').textContent = actionType === 'cancel' ? 'cancel' : 'return';
            
            // Retrieve item details from the table row
            const row = document.querySelector(`[onclick="openItemModal('${orderId}', '${itemId}', '${actionType}')"]`).closest('tr');
            const productName = row.querySelector('td:nth-child(1)').textContent;
            const itemStatus = row.querySelector('td:nth-child(3)').textContent;
            const totalPrice = row.querySelector('td:nth-child(4)').textContent;
    
            // Display item details in the modal
            document.getElementById('modalProductName').textContent = productName;
            document.getElementById('modalItemStatus').textContent = itemStatus;
            document.getElementById('modalTotalPrice').textContent = totalPrice;
    
            // Show the modal
            const itemModal = new bootstrap.Modal(document.getElementById('itemModal'));
            itemModal.show();
        }
    
        function confirmItemAction() {
            // Send the fetch request to cancel or return the item
            fetch(actionUrl, { method: 'POST' })
                .then(response => response.json())
                .then(data => {
                    if (data.success) {
                        // Hide the corresponding action button
                        const actionButton = document.querySelector(`[onclick="openItemModal('${data.orderId}', '${data.itemId}', '${data.actionType}')"]`);
                        if (actionButton) {
                            actionButton.style.display = 'none';
                        }
    
                        // Update status text in the table
                        const statusCell = actionButton.closest('tr').querySelector('td:nth-child(3)');
                        statusCell.textContent = data.actionType === 'cancel' ? 'Canceled' : 'Returned';
    
                        // Display a notification
                        displayNotification(data.message, true);
                    } else {
                        displayNotification(data.message, false);
                    }
                })
                .catch(error => {
                    console.error('Error:', error);
                    displayNotification('An error occurred while processing the request.', false);
                });
    
            // Close the modal after confirming action
            const itemModal = bootstrap.Modal.getInstance(document.getElementById('itemModal'));
            itemModal.hide();
        }
    
        // Function to display notifications
        function displayNotification(message, isSuccess) {
            const notification = document.getElementById('notification');
            notification.innerText = message;
            notification.className = `alert ${isSuccess ? 'alert-success' : 'alert-danger'}`;
            notification.style.display = 'block';
    
            // Hide the notification after a few seconds
            setTimeout(() => {
                notification.style.display = 'none';
            }, 3000);
        }
    </script>
    
    
    
    

    
