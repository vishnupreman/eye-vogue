        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <table class="table shopping-summery text-center clean">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col">Image</th>
                                        <th scope="col">Name</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Quantity</th>
                                        <th scope="col">Subtotal</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% cartItems.forEach(element => { %>
                                        <tr>
                                            <td class="image product-thumbnail">
                                                <% if (element.product && element.product.colors && element.product.colors[element.color] && element.product.colors[element.color].images && element.product.colors[element.color].images.length > 0) { %>
                                                    <img src="/uploads/<%= element.product.colors[element.color].images[0] %>" alt="Product Image">
                                                <% } else { %>
                                                    <img src="/uploads/default-image.jpg" alt="Default Image">
                                                <% } %>
                                            </td>
                                            <td class="product-des product-name">
                                                <h5 class="product-name">
                                                    <% if (element.product && element.product.name) { %>
                                                        <a href="shop-product-right.html"><%= element.product.name %></a>
                                                    <% } else { %>
                                                        <span>Product name not available</span>
                                                    <% } %>
                                                </h5>
                                                <p class="font-xs"><%= element.product ? element.product.description : 'Description not available' %></p>
                                            </td>
                                            <td class="price" data-item-id="<%= element.product ? element.product._id : '' %>-<%= element.color %>">
                                                <span id="price-<%= element.product ? element.product._id : '' %>-<%= element.color %>">
                                                    Rs.<%= (element.product && element.product.discountedPrice) || element.price %>
                                                </span>
                                            </td>
                                            <td class="text-center" data-title="Stock">
                                                <div class="detail-qty border radius m-auto">
                                                    <a href="#" onclick="qtydown('<%= element.product ? element.product._id : '' %>', '<%= element.color %>')" class="qty-down">
                                                        <i class="fi-rs-angle-small-down"></i>
                                                    </a>
                                                    <span class="qty-val" id="quantity-<%= element.product ? element.product._id : '' %>-<%= element.color %>"><%= element.quantity %></span>
                                                    <a href="#" onclick="qtyup('<%= element.product ? element.product._id : '' %>', '<%= element.color %>')" class="qty-up">
                                                        <i class="fi-rs-angle-small-up" id="qty-up-<%= element.product ? element.product._id : '' %>-<%= element.color %>"></i>
                                                    </a>
                                                </div>
                                                <span class="quantity-message" id="quantity-message-<%= element.product ? element.product._id : '' %>-<%= element.color %>" style="display: none; color: red;">Cannot exceed 10</span>
                                            </td>
                                            <td class="text-right" data-item-id="<%= element.product ? element.product._id : '' %>-<%= element.color %>">
                                                <span id="subtotal-<%= element.product ? element.product._id : '' %>-<%= element.color %>">
                                                    Rs.<%= (element.product && element.product.discountedPrice || element.price) * element.quantity %>
                                                </span>
                                            </td>
                                            <td class="action" data-title="Remove">
                                                <a href="#" onclick="removeItem('<%= element.product ? element.product._id : '' %>')" class="text-muted">
                                                    <i class="fi-rs-trash"></i>
                                                </a>
                                            </td>
                                        </tr>   
                                    <% }); %>
                                   
                                </tbody>
                                
                                
                                
                                
                                
                                
                            </table>
                        </div>
                      
                        <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>
                        <div class="row mb-50">
                            
                            <div class="col-lg-6 col-md-12">
                                <div class="border p-md-4 p-30 border-radius cart-totals">
                                    <div class="heading_s1 mb-3">
                                        <h4>Cart Totals</h4>
                                        <p id="mac-react"></p>
                                    </div>
                                    <div class="table-responsive">
                                        <table class="table">
                                            <tbody>
                                                <tr>
                                                    <td class="cart_total_label">Cart Subtotal</td>
                                                    <td class="cart_total_amount"><span class="font-lg fw-900 text-brand"></span></td>
                                                </tr>
                                                <tr>
                                                    <td class="cart_total_label">Shipping</td>
                                                    <td class="cart_total_amount"> <i class="ti-gift mr-5"></i> Free Shipping</td>
                                                </tr>
                                                <tr>
                                                    <td class="cart_total_label">Total</td>
                                                    <td class="cart_total_amount"><strong><span class="font-xl fw-900 text-brand" id="cartTotal"><%= cart.totalPrice %></span></strong></td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <a href="/user/checkout" class="btn "> <i class="fi-rs-box-alt mr-10"></i> Proceed To CheckOut</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
