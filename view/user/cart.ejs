    <script>
      function qtyup(productId, color) {
    const quantityElement = document.getElementById(`quantity-${productId}-${color}`);
    const quantityMessageElement = document.getElementById(`quantity-message-${productId}-${color}`);
    const qtyUpButton = document.getElementById(`qty-up-${productId}-${color}`);
    const currentQuantity = parseInt(quantityElement.textContent, 10);

    if (currentQuantity < 10) {
        const newQuantity = currentQuantity + 1;
        updateCartQuantity(productId, color, newQuantity);
        quantityMessageElement.style.display = 'none';
    }

    if (currentQuantity + 1 === 10) {
        quantityMessageElement.style.display = 'inline';
        qtyUpButton.style.pointerEvents = 'none'; // Disables the button
        qtyUpButton.classList.add('disabled'); // Optionally add a class for styling
    }
}

function qtydown(productId, color) {
    const quantityElement = document.getElementById(`quantity-${productId}-${color}`);
    const qtyUpButton = document.getElementById(`qty-up-${productId}-${color}`);
    const currentQuantity = parseInt(quantityElement.textContent, 10);

    if (currentQuantity > 1 && currentQuantity <= 10) {
        const newQuantity = currentQuantity - 1;
        updateCartQuantity(productId, color, newQuantity);

        qtyUpButton.style.pointerEvents = 'auto'; // Re-enables the button
        qtyUpButton.classList.remove('disabled');
    }
}
function updateCartQuantity(productId, color, newQuantity) {
    fetch('/user/cart/updatequantity', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ productId, color, newQuantity })
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            const quantityElement = document.getElementById(`quantity-${productId}-${color}`);
            quantityElement.textContent = newQuantity;

            const priceElement = document.getElementById(`price-${productId}-${color}`);
            const price = parseFloat(priceElement.textContent.replace('Rs.', ''));
            const subtotalElement = document.getElementById(`subtotal-${productId}-${color}`);
            

            subtotalElement.textContent = `Rs.${(price * newQuantity).toFixed(2)}`;
            const totalAmount = document.getElementById('cartTotal')
            console.log(data);
            
            totalAmount.textContent= `Rs.${data.cart.totalPrice}`
        }
    })
    .catch(error => {
        console.error('Error updating quantity:', error);
    });
}

function removeItem(productId) {
    fetch('/user/cart/removeitem', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify({ productId })
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            const row = document.querySelector(`a[onclick="removeItem('${productId}')"]`).closest('tr');
            row.parentNode.removeChild(row);
        }
    })
    .catch(error => {
        console.error('Error removing item:', error);
    });
}

    </script>
